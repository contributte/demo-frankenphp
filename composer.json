{
  "name": "contributte/demo-frankenphp",
  "description": "Nette + FrankenPHP by Felix",
  "keywords": [
    "nette",
    "frankenphp"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "contributte/nella": "^0.3.0"
  },
  "require-dev": {
    "contributte/dev": "^0.5",
    "symfony/thanks": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "symfony/thanks": true
    }
  }
}
